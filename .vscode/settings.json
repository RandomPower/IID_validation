{
    "autoDocstring.docstringFormat": "numpy",
    "python.analysis.typeCheckingMode": "basic",
    "python.analysis.autoImportCompletions": true,
    "isort.check": true,
    "isort.args": [
        "--profile",
        "black"
    ],
    "isort.severity": {
        "W": "Warning",
        "E": "Error"
    },
    "flake8.args": [
        "--max-line-length=120",
        "--ignore=E203,E701,W503"
    ],
    "black-formatter.args": [
        "--line-length",
        "119"
    ],
    "[python]": {
        "editor.rulers": [
            120
        ]
    }
}